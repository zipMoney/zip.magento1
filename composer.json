{
    "name": "zipmoney/magento-zipmoneypayment",
    "description": "Magento Extension for enabling zipMoney Payment",
    "require": {
        "zipMoney/zipMoney-MerchantApi-php": "dev-feature/api"
    },
    "authors": [
        {
            "name": "zipMoney Payments",
            "email": "sagar.bhandari@zipmoney.com.au"
        }
    ],    
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { 
          "zipMoney\\" : "vendor/zipMoney/zipMoney-MerchantApi-php/lib/"
         }
    },
    "autoload-dev": {
        "psr-4": { "zipMoneyPHP\\" : "zipMoney/zipMoney-MerchantApi-php/test/" }
    },
    "repositories": [
        {
            "type":"package",
            "package": {
              "name": "zipMoney/zipmoney-apiv2-php",
              "version": "dev-feature/apiv2",
              "source": {
                  "url": "https://github.com/zipmoney/zipmoney-apiv2-php",
                  "type": "git",
                  "reference": "feature/apiv2"
                }
            }
        },
        {
            "type":"package",
            "package": {
              "name": "zipMoney/zipMoney-MerchantApi-php",
              "version": "dev-feature/api",
              "source": {
                  "url": "https://github.com/zipmoney/zipMoney-MerchantApi-php",
                  "type": "git",
                  "reference": "feature/api"
                }
            }
        }
    ]
}
